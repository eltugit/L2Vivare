<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="list">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="zone" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="playerTeleport">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="locationTeleportPlayer" minOccurs="4" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="x" type="xs:int"></xs:attribute>
                                                <xs:attribute name="y" type="xs:int"></xs:attribute>
                                                <xs:attribute name="z" type="xs:int"></xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>

                            <xs:element name="locationSpawnRaidBoss" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:attribute name="bossId" type="xs:int"></xs:attribute>
                                    <xs:attribute name="x" type="xs:int"></xs:attribute>
                                    <xs:attribute name="y" type="xs:int"></xs:attribute>
                                    <xs:attribute name="z" type="xs:int"></xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute name="npcId" type="xs:int"></xs:attribute>
                        <xs:attribute name="name" type="xs:string"></xs:attribute>
                        <xs:attribute name="imageName" type="xs:string"></xs:attribute>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>